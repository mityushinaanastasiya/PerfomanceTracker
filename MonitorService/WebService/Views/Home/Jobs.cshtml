@using WebService.DataAccess.Models
@model List<Job>

<h3>Jobs</h3>
<hr width="30%" align="left" />
<br />
    <table class="table table-striped table-bordered table-hover">
        <thead class="thead-dark text-white">
            <tr>
                <th>Extension Service Name</th>
                <th>Extension Type</th>
                <th>Library Name</th>
                <th>Job State</th>
                <th>Final Status</th>
                <th>Queue Time</th>
                <th>Messages</th>
                <th></th>
            </tr>
        </thead>

        <tbody>
            @foreach (var job in Model)
            {
                <tr>
                        <th>@job.ExtensionServiceName</th>
                        <th>@job.ExtensionType</th>
                        <th>@job.LibraryName</th>
                        <th>@job.JobState</th>
                        <th>@job.FinalStatus</th>
                        <th>@job.QueueTime</th>
                        <th>@job.Messages</th>
                        <th><a asp-area="" asp-controller="Jobs" asp-action="Index" class="text-dark">See more</a></th>
                    </tr>
                }

            </tbody>
    </table>


    



